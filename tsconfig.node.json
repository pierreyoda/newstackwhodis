{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ESNext"],
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "module": "ESNext",
    "resolveJsonModule": true,
    "baseUrl": "."
  },
  "exclude": ["node_modules"]
}
