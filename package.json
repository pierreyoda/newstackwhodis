{
  "name": "praca-website",
  "version": "0.1.0",
  "private": true,
  "author": "pierreyoda <pierreyoda@users.noreply.github.com>",
  "type": "module",
  "scripts": {
    "fetch:projects": "yarn cross-env DEBUG=praca:* TS_NODE_PROJECT=./tsconfig.node.json yarn node --experimental-specifier-resolution=node --loader ts-node/esm ./src/api/fetcher.ts",
    "fetch": "yarn fetch:projects",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "type": "yarn tsc --noEmit",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "yarn jest"
  },
  "dependencies": {
    "d3-path": "3.0.1",
    "d3-selection": "3.0.0",
    "debug": "4.3.4",
    "lodash.orderby": "4.6.0",
    "node-emoji": "1.11.0"
  },
  "devDependencies": {
    "@octokit/rest": "19.0.5",
    "@sveltejs/adapter-static": "1.0.0-next.49",
    "@sveltejs/kit": "1.0.0-next.584",
    "@tailwindcss/typography": "0.5.8",
    "@types/d3-path": "3.0.0",
    "@types/d3-selection": "3.0.3",
    "@types/debug": "4.1.7",
    "@types/jest": "29.2.4",
    "@types/lodash.orderby": "4.6.7",
    "@types/node": "18.11.14",
    "@types/node-emoji": "1.8.2",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "autoprefixer": "10.4.13",
    "chalk": "5.2.0",
    "cross-env": "7.0.3",
    "cssnano": "5.1.14",
    "dotenv": "16.0.3",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "mdsvex": "0.10.6",
    "postcss": "8.4.20",
    "postcss-load-config": "4.0.1",
    "postcss-nested": "6.0.0",
    "prettier": "2.8.1",
    "prettier-plugin-svelte": "2.9.0",
    "remark-emoji": "3.0.2",
    "svelte": "3.54.0",
    "svelte-check": "2.10.2",
    "svelte-preprocess": "5.0.0",
    "svelte-range-slider-pips": "2.1.1",
    "tailwindcss": "3.2.4",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "tslib": "2.4.1",
    "typescript": "4.9.4",
    "unist-util-visit": "^4.1.1",
    "vite": "4.0.1"
  }
}
