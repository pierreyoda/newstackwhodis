{
  "name": "praca-website",
  "version": "0.1.0",
  "private": true,
  "author": "pierreyoda <pierreyoda@users.noreply.github.com>",
  "type": "module",
  "scripts": {
    "fetch:projects": "yarn cross-env DEBUG=praca:* TS_NODE_PROJECT=./tsconfig.node.json yarn node --experimental-specifier-resolution=node --loader ts-node/esm ./src/api/fetcher.ts",
    "fetch": "yarn fetch:projects",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "yarn jest"
  },
  "dependencies": {
    "debug": "4.3.2",
    "lodash.orderby": "4.6.0",
    "node-emoji": "1.11.0"
  },
  "devDependencies": {
    "@octokit/rest": "18.12.0",
    "@sveltejs/kit": "next",
    "@tailwindcss/typography": "0.4.1",
    "@types/debug": "4.1.7",
    "@types/lodash.orderby": "4.6.6",
    "@types/node": "16.11.10",
    "@types/node-emoji": "1.8.1",
    "@types/tailwindcss": "2.2.4",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "autoprefixer": "10.4.0",
    "chalk": "4.1.2",
    "cross-env": "7.0.3",
    "cssnano": "5.0.11",
    "dotenv": "10.0.0",
    "eslint": "8.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-svelte3": "3.2.1",
    "husky": "7.0.4",
    "mdsvex": "^0.9.8",
    "postcss": "8.4.1",
    "postcss-load-config": "3.1.0",
    "postcss-nested": "5.0.6",
    "prettier": "2.4.1",
    "prettier-plugin-svelte": "2.5.0",
    "remark-emoji": "^3.0.2",
    "svelte": "3.44.2",
    "svelte-check": "2.2.10",
    "svelte-preprocess": "4.9.8",
    "tailwindcss": "2.2.19",
    "ts-node": "10.4.0",
    "tslib": "2.3.1",
    "typescript": "4.5.2"
  }
}
